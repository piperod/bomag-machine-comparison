<!--START TAKEOVER-->
<script>

	var enableTakeOver = false; //enable or disable takeover system

	//switch to false to enable takeover!
	var isTakeOverVisited = false;

	function updateTakeOver() {
		if (enableTakeOver === false) {return;}
		if (isTakeOverVisited) {
			$(".container.body-wrapper").show();
			$("#takeover").slideUp();
			// $("#takeover").hide();
			$('body').css('overflow', 'auto');
		} else {
			// $(".container.body-wrapper").show();
			$("#takeover").slideDown();

			$('body').css('overflow', 'hidden');
		}
		//$("header").fadeOut();
	}

	$(function() {

		isTakeOverVisited = (localStorage.getItem("isTakeOverVisited2025"));


		//take over script
		updateTakeOver();

		$("#takeover .go-to-home, #takeover .read-more").click(function() {
			localStorage.setItem("isTakeOverVisited2025", true);
			isTakeOverVisited = true;
			updateTakeOver();
		});


	});
</script>
<!--END TAKEOVER-->


